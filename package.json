{
    "name": "refal-5-lambda",
    "displayName": "Refal-5λ",
    "publisher": "gdvfox",
    "description": "Refal-5λ support for Visual Studio Code",
    "author": {
        "name": "Daniil Gavrilovsky"
    },
    "license": "MIT",
    "icon": "images/icon.png",
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/GDVFox/vscode-refal-5-lambda.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "refal-5-lambda",
                "aliases": [
                    "Refal-5λ",
                    "refal-5-lambda"
                ],
                "extensions": [
                    ".ref",
                    ".refi"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "refal-5-lambda",
                "scopeName": "source.refal-5-lambda",
                "path": "./syntaxes/refal-5-lambda.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.cpp": "cpp"
                }
            }
        ]
    }
}
